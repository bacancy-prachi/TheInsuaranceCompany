<p id="notice"><%= notice %></p>

<h1>Quotations</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Address</th>
      <th>Postal code</th>
      <th>City</th>
      <th>Phone</th>
      <th>Province</th>
      <th>Municiple evaluation of property</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quotations.each do |quotation| %>
      <tr>
        <td><%= quotation.email %></td>
        <td><%= quotation.first_name %></td>
        <td><%= quotation.last_name %></td>
        <td><%= quotation.address %></td>
        <td><%= quotation.postal_code %></td>
        <td><%= quotation.city %></td>
        <td><%= quotation.phone %></td>
        <td><%= quotation.province %></td>
        <td><%= quotation.municiple_evaluation_of_property %></td>
        <td><%= link_to 'Show', quotation %></td>
        <td><%= link_to 'Edit', edit_quatation_path(quotation) %></td>
        <td><%= link_to 'Destroy', quotation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quotation', new_quatation_path %>
